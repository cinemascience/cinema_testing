<?xml version="1.0" ?>
<pqevents>
  <!-- load state -->
  <pqevent object="pqClientMainWindow/menubar/menu_File" command="activate" arguments="actionFileLoadServerState" />
  <pqevent object="pqClientMainWindow/FileLoadServerStateDialog" command="filesSelected" arguments="test/time_source.pvsm" />
  <pqevent object="LoadStateOptionsDialog/widget/OKButton" command="activate" arguments="" />

  <!-- select element in pipeline browser -->
  <pqevent object="pqClientMainWindow/pipelineBrowserDock/pipelineBrowser" command="mousePress" arguments="1,1,0,50,11,/0:0/0:0" />
  <pqevent object="pqClientMainWindow/pipelineBrowserDock/pipelineBrowser" command="mouseRelease" arguments="1,0,0,50,11,/0:0/0:0" />

  <!-- activate catalyst export window -->
  <pqevent object="pqClientMainWindow/menubar" command="activate" arguments="menu_Catalyst" />
  <pqevent object="pqClientMainWindow/menubar/menu_Catalyst" command="activate" arguments="actionCatalystConfigure" />

  <!-- export spec d table -->
  <pqevent object="pqClientMainWindow/catalystInspectorDock/CatalystExportInspector/advanced" command="set_boolean" arguments="true" />
  <pqevent object="pqClientMainWindow/catalystInspectorDock/CatalystExportInspector/container/1pqProxyWidget0/SaveDTable/CheckBox" command="set_boolean" arguments="true" />

  <!-- export geometry -->
  <pqevent object="pqClientMainWindow/catalystInspectorDock/CatalystExportInspector/filterFormat" command="activated" arguments="XMLMultiBlockDataWriter" />
  <pqevent object="pqClientMainWindow/catalystInspectorDock/CatalystExportInspector/filterExtract" command="set_boolean" arguments="true" />

  <!-- set root directory for batch export -->
  <pqevent object="pqClientMainWindow/catalystInspectorDock/CatalystExportInspector/container/1pqProxyWidget0/RootDirectory/RootDirectory" command="set_string" arguments="data/batch/test_cdb_geom.cdb" />

  <!-- export catalyst script -->
  <pqevent object="pqClientMainWindow/menubar/menu_Catalyst" command="activate" arguments="actionExportCatalyst" />
  <pqevent object="pqClientMainWindow/menubar/menu_Catalyst/ExportCoprocessingStateFileDialog" command="filesSelected" arguments="data/test_cdb_geom.py" />

  <!-- set root directory for interactive export -->
  <pqevent object="pqClientMainWindow/catalystInspectorDock/CatalystExportInspector/container/1pqProxyWidget0/RootDirectory/RootDirectory" command="set_string" arguments="data/interactive/test_cdb_geom.cdb" />

  <!-- export cinema database -->
  <pqevent object="pqClientMainWindow/menubar" command="activate" arguments="menu_Catalyst" />
  <pqevent object="pqClientMainWindow/menubar/menu_Catalyst" command="activate" arguments="actionExportImmediate" />
</pqevents>
